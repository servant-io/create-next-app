{
  "name": "create-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:api": "tsx scripts/test-api.ts",
    "test:load": "tsx scripts/test-load.ts",
    "test:coverage": "vitest run --coverage",
    "postinstall": "servant link-agents && servant link-skills && servant docs update"
  },
  "dependencies": {
    "@servant-io/actions": "0.5.0",
    "@servant-io/agents": "0.5.0",
    "@servant-io/cli": "0.5.0",
    "@servant-io/skills": "0.5.0",
    "next": "16.0.10",
    "react": "19.2.1",
    "react-confetti": "^6.4.0",
    "react-dom": "19.2.1",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-v8": "^2.1.4",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.19.1",
    "typescript": "^5",
    "vitest": "^2.1.4"
  }
}
